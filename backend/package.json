{
  "name": "backend",
  "version": "0.0.0",
  "scripts": {
    "start": "node -r module-alias/register ./dist --env=production",
    "dev": "nodemon",
    "build": "tsc"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts, html",
    "ignore": [
      "src/public"
    ],
    "exec": "./node_modules/.bin/ts-node --files -r tsconfig-paths/register ./src"
  },
  "_moduleAliases": {
    "@src": "dist"
  },
  "engines": {
    "node": ">=8.10.0"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "deepai": "^1.0.23",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "inserturlparams": "^1.0.1",
    "jsonfile": "^6.1.0",
    "module-alias": "^2.2.3",
    "sqlite3": "^5.1.6",
    "ts-command-line-args": "^2.5.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/find": "^0.2.1",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^20.5.0",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6"
  }
}
